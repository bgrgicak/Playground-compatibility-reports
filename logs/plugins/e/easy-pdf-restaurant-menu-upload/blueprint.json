{
  "landingPage": "/wp-admin/admin.php?page=easy-pdf-restaurant-menu",
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "features": {
    "networking": true
  },
  "login": true,
  "plugins": [
    "easy-pdf-restaurant-menu-upload"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "setSiteOptions",
      "options": {
        "blogname": "Easy Restaurant Menu Download"
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "easy-pdf-restaurant-menu-upload"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
