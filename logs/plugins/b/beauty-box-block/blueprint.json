{
  "landingPage": "/wp-admin/edit.php",
  "preferredVersions": {
    "php": "7.4",
    "wp": "5.9"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "beauty-box-block"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
