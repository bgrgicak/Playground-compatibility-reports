{
  "plugins": [
    "woocommerce",
    "custom-product-type-for-woocommerce"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "installTheme",
      "themeData": {
        "resource": "wordpress.org/themes",
        "slug": "twentynineteen"
      }
    },
    {
      "step": "importFile",
      "fileType": "customPostType",
      "filePath": "path/to/custom-product-types-file.json"
    },
    {
      "step": "importFile",
      "fileType": "woocommerceProducts",
      "filePath": "http://wpsaad.com/wp-content/uploads/2025/04/wc-product-export-23-4-2025-1745412506318.csv"
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "custom-product-type-for-woocommerce"
      },
      "options": {
        "activate": true
      }
    }
  ],
  "siteOptions": {
    "blogname": "Custom Product Type for WooCommerce",
    "blogdescription": " Add-Ons, Data, Options, Layouts, Booking & Appointments"
  },
  "constants": {
    "WP_DEBUG": false
  }
}
