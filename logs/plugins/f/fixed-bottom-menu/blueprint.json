{
  "landingPage": "/wp-admin/admin.php?page=fixedbottommenu",
  "preferredVersions": {
    "php": "8.0",
    "wp": "6.6"
  },
  "login": true,
  "plugins": [
    "fixed-bottom-menu"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "fixed-bottom-menu"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
