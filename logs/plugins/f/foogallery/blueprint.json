{
  "landingPage": "/wp-admin/edit.php?post_type=foogallery",
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "features": {
    "networking": true
  },
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "FOOPLUGINS_FREEMIUS_ANONYMOUS": true
      }
    },
    {
      "step": "login"
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "foogallery"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "runPHP",
      "code": "<?php foogallery_create_demo_content(); ?>"
    }
  ]
}
