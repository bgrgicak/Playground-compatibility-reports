{
  "landingPage": "/wp-admin/admin.php?page=wc-settings&tab=shipping&instance_id=3",
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "features": {
    "networking": true
  },
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "importWordPressFiles",
      "wordPressFilesZip": {
        "resource": "url",
        "url": "https://files.gravyzzap.com/wporg-wbs-preview.zip"
      }
    },
    {
      "step": "activatePlugin",
      "pluginPath": "weight-based-shipping-for-woocommerce/plugin.php"
    },
    {
      "step": "login",
      "username": "demo",
      "password": "demo"
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "weight-based-shipping-for-woocommerce"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
