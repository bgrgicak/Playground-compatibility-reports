{
  "landingPage": "/",
  "preferredVersions": {
    "php": "8.2",
    "wp": "6.6"
  },
  "phpExtensionBundles": [
    "kitchen-sink"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "wp-logged-in-only"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
