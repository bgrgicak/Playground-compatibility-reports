{
  "landingPage": "/wp-admin/post-new.php",
  "plugins": [
    "safelayout-brilliant-buttons"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "safelayout-brilliant-buttons"
      },
      "options": {
        "activate": true
      }
    }
  ],
  "preferredVersions": {
    "php": "8.0",
    "wp": "latest"
  },
  "features": {
    "networking": true
  },
  "login": true
}
