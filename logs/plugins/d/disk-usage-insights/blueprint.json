{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/wp-admin/tools.php?page=disk-usage-insights",
  "login": true,
  "features": {
    "networking": true
  },
  "plugins": [
    "disk-usage-insights"
  ],
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "activatePlugin",
      "pluginName": "Disk Usage Insights",
      "pluginPath": "/wordpress/wp-content/plugins/disk-usage-insights"
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "disk-usage-insights"
      },
      "options": {
        "activate": true
      }
    }
  ]
}
