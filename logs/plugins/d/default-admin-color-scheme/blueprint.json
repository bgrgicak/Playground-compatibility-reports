{
  "landingPage": "/wp-admin/options-general.php#users_can_change_color_scheme",
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_AUTO_UPDATE_CORE": false,
        "DISABLE_WP_CRON": true
      }
    },
    {
      "step": "enableMultisite"
    },
    {
      "step": "login",
      "username": "admin",
      "password": "password"
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "default-admin-color-scheme"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginZipFile": {
        "resource": "wordpress.org/plugins",
        "slug": "user-switching"
      }
    },
    {
      "step": "runPHP",
      "code": "<?php require '/wordpress/wp-load.php'; wp_insert_user(['user_pass' => 'password','user_login' => 'test', 'display_name' => 'Test User', 'user_email' => 'test@localhost.com', 'role' => 'editor',]);"
    }
  ]
}
